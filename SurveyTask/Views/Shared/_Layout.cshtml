@using Newtonsoft.Json

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Security-Policy" content="upgrade-insecure-requests">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>
    <link rel="icon" href="~/img/cropped-Vl_final-32x32.png" sizes="32x32">

    <!-- STYLES -->

    <link href="~/wwwroot/js/jtable/Content/themes/metroblue/jquery-ui.css" rel="stylesheet" type="text/css" />
    <link href="~/wwwroot/js/jtable/Scripts/jtable/themes/metro/blue/jtable.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="~/wwwroot/vendor/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="~/wwwroot/fonts/fonts.css" />
    <link rel="stylesheet" href="~/wwwroot/fonts/font-awesome-4.7.0/css/font-awesome.min.css" />
    <link href="~/wwwroot/css/timepicker/jquery-ui-timepicker-addon.css" rel="stylesheet" />
    <link href="~/wwwroot/css/material-dashboard.css?v=2.1.2" rel="stylesheet" />
    <link href="~/wwwroot/demo/demo.css" rel="stylesheet" />
    <link href="~/wwwroot/css/site.css" rel="stylesheet" />

    <!-- Jquery -->
    @*the jquery must be here to be able to execute the scribts in the partial view (Components) that uses jquery Like (ExecludeFilters - IncludeFilters - etc..)*@
    <script type="text/javascript" src="~/js/core/jquery-1.10.2.min.js"></script>
    <script>
    </script>
</head>
<body>
    <div class="wrapper">
        <div class="sidebar" data-color="purple" data-background-color="white">
            <!--
                Tip 1: You can change the color of the sidebar using: data-color="purple | azure | green | orange | danger"

                Tip 2: you can also add an image using data-image tag
            -->
            <div class="logo" style="text-align:center;">
                <a target="_blank">Admin Portal</a>
            </div>
            <div class="sidebar-wrapper">
                <ul class="nav" style="text-align:center;">
                    <li style="margin:auto">Survey</li>
                </ul>
            </div>
        </div>
        <div class="main-panel">
            <!-- Navbar -->

            <nav class="navbar navbar-expand-lg navbar-transparent navbar-absolute fixed-top ">
                <div class="container-fluid">
                    <div class="navbar-wrapper">
                        <a class="navbar-brand" href="javascript:;">@ViewData["Title"]</a>
                    </div>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="navbar-toggler-icon icon-bar"></span>
                        <span class="navbar-toggler-icon icon-bar"></span>
                        <span class="navbar-toggler-icon icon-bar"></span>
                    </button>
                    <div class="collapse navbar-collapse justify-content-end">
                        <form class="navbar-form">
                        </form>
                        <ul class="navbar-nav">
                            <li class="nav-item dropdown">
                                <a class="nav-link" href="javascript:;" id="navbarDropdownProfile" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <i class="material-icons">person</i>
                                    <p class="d-lg-none d-md-block">
                                        Account
                                    </p>
                                </a>
                                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdownProfile">
                                    @*<a class="dropdown-item" href="#">Profile</a>
                                        <a class="dropdown-item" href="#">Settings</a>
                                        <div class="dropdown-divider"></div>*@
                                    <a class="dropdown-item" asp-controller="Account" asp-action="LogOut">Log out</a>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
            <!-- End Navbar -->
            <div class="content">
                <div class="container-fluid">
                    <div class="col-md-12">
                    </div>
                    <div class="col-sm-12">
                            <div id="divMessage" style="display:none;margin-top:0px; margin-bottom:30px;text-align:center" role="alert">
                                <span style="float: right; cursor: pointer" onclick="CloseMessage('divMessage')" title="Hide the message">×</span>
                                <p id="messageText" style="align-content:center;font-weight : bolder"></p>
                            </div>
                        @RenderBody()
                    </div>
                    <footer class="footer">
                        <div class="container-fluid">
                            <div class="copyright">
                                <span id="lblDevelopedByVL" style="color : black">
                                    Developed By
                                </span>
                                <a href="" target="_blank">Ahmed Sakr.</a>
                                <span id="lblDevelopedBySakr" style="color : black">
                                    Copyright &copy;
                                    <script>
                                        document.write(new Date().getFullYear())
                                    </script>
                                    , All Rights Reserved to
                                </span>
                                <a href="" target="_blank">Ahmed Sakr.</a>
                            </div>
                            <!-- your footer here -->
                        </div>
                    </footer>
                </div>
            </div>
        </div>
    </div>
    <!-- SCRIPTS -->
    <script type="text/javascript" src="~/wwwroot/js/core/jquery-1.10.2.min.js"></script>
    <script type="text/javascript" src="~/wwwroot/js/core/jquery-ui-1.9.2.js"></script>
    <script type="text/javascript" src="~/wwwroot/js/core/popper.min.js"></script>
    <script type="text/javascript" src="~/wwwroot/js/core/bootstrap-material-design.min.js"></script>
    <script type="text/javascript" src="~/wwwroot/js/plugins/perfect-scrollbar.jquery.min.js"></script>
    <script type="text/javascript" src="~/wwwroot/js/material-dashboard.js?v=2.1.2"></script>
    <script type="text/javascript" src="~/wwwroot/js/jtable/Scripts/jtable/jquery.jtable.js"></script>
    <script type="text/javascript" src="~/wwwroot/js/timepicker/jquery-ui-timepicker-addon.js"></script>
    <script type="text/javascript" src="~/wwwroot/js/core/moment.min.js"></script>
    <script type="text/javascript" src="~/wwwroot/js/site.js"></script>
    @RenderSection("Scripts", false)
</body>
</html>
